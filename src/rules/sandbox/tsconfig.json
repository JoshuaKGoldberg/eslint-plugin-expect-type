{
	"compilerOptions": {
		"lib": ["es2017"],
		"module": "commonjs",
		"moduleResolution": "node",

		// Testing that these don't get passed through as issues
		// https://github.com/ibezkrovnyi/eslint-plugin-expect-type/issues/16
		"noUnusedLocals": true,
		"noUnusedParameters": true,

		"skipLibCheck": true,
		"strict": true,
		"target": "es2017"
	},
	"files": ["file.ts", "package/foo.ts"]
}
