{
  "name": "eslint-plugin-expect-type",
  "version": "0.0.7",
  "description": "ESLint plugin with $ExpectType, $ExpectError and $ExpectTypeSnapshot type assertions",
  "author": {
    "name": "Josh Goldberg"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier \"**/*.{js,json,md,ts,yml}\"",
    "format:check": "npm run format -- --check",
    "format:write": "npm run format -- --write",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "prepublishOnly": "tsc",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoshuaKGoldberg/eslint-plugin-expect-type.git"
  },
  "keywords": [
    "dtslint",
    "d.ts",
    "eslint",
    "tslint",
    "plugin",
    "config",
    "rule",
    "typescript",
    "ts",
    "Expect",
    "Assert",
    "Type",
    "Snapshot",
    "$ExpectError",
    "$ExpectType",
    "$ExpectTypeSnapshot"
  ],
  "peerDependencies": {
    "eslint": "^7.32.0",
    "typescript": ">=4.4.3"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.33.0",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/eslint": "^7.28.0",
    "@types/estree": "^0.0.50",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "dedent": "^0.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-plugin": "^3.6.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.4",
    "json-schema": "^0.3.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "tsutils": "^3.21.0",
    "typescript": "^3.7.5"
  }
}
